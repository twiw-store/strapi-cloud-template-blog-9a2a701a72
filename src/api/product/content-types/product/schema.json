{
  "kind": "collectionType",
  "collectionName": "products",
  "info": {
    "singularName": "product",
    "pluralName": "products",
    "displayName": "Product"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string"
    },

    "title_ru": {
      "type": "string"
    },
    "title_en": {
      "type": "string"
    },
    "title_fr": {
      "type": "string"
    },
    "title_es": {
      "type": "string"
    },

    "slug": {
      "type": "uid",
      "targetField": "title"
    },

    "price": {
      "type": "decimal",
      "options": {
        "precision": 10,
        "scale": 2
      }
    },

    "compareAtPrice": {
      "type": "decimal",
      "required": false,
      "default": null,
      "options": {
        "precision": 10,
        "scale": 2
      },
      "configurable": true,
      "pluginOptions": {
        "content-manager": {
          "visible": true
        }
      }
    },

    "saleStart": {
      "type": "datetime",
      "required": false,
      "default": null,
      "configurable": true,
      "pluginOptions": {
        "content-manager": {
          "visible": true
        }
      }
    },

    "saleEnd": {
      "type": "datetime",
      "required": false,
      "default": null,
      "configurable": true,
      "pluginOptions": {
        "content-manager": {
          "visible": true
        }
      }
    },

    "description": {
      "type": "text"
    },
    "description_ru": {
      "type": "text"
    },
    "description_en": {
      "type": "text"
    },
    "description_fr": {
      "type": "text"
    },
    "description_es": {
      "type": "text"
    },

    "images": {
      "type": "media",
      "multiple": true,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },

    "category": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::category.category",
      "inversedBy": "products"
    },

    "colors": {
      "type": "string"
    },

    "dimensions": {
      "type": "component",
      "repeatable": false,
      "component": "product.dimensions"
    },

    "variants": {
      "type": "component",
      "repeatable": true,
      "component": "product.variant"
    },

    "look": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::product.product",
      "mappedBy": "products"
    },

    "products": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::product.product",
      "inversedBy": "look"
    },

    "details": {
      "type": "text"
    },
    "details_ru": {
      "type": "text"
    },
    "details_en": {
      "type": "text"
    },
    "details_fr": {
      "type": "text"
    },
    "details_es": {
      "type": "text"
    },

    "sizeInfo": {
      "type": "string"
    },
    "sizeInfo_ru": {
      "type": "string"
    },
    "sizeInfo_en": {
      "type": "string"
    },
    "sizeInfo_fr": {
      "type": "string"
    },
    "sizeInfo_es": {
      "type": "string"
    },

    "care": {
      "type": "string"
    },
    "care_ru": {
      "type": "string"
    },
    "care_en": {
      "type": "string"
    },
    "care_fr": {
      "type": "string"
    },
    "care_es": {
      "type": "string"
    },

    "about": {
      "type": "string"
    },
    "about_ru": {
      "type": "string"
    },
    "about_en": {
      "type": "string"
    },
    "about_fr": {
      "type": "string"
    },
    "about_es": {
      "type": "string"
    }
  }
}
